{
  "name": "touch-tech-test",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "test": "jest",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
  },
  "dependencies": {
    "pinia": "^2.1.3",
    "vue": "^3.3.2",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.13",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.0",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vue-tsc": "^1.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|js?|ts?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
